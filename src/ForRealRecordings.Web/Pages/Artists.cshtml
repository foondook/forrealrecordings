@page "{name?}"
@inject OrchardCore.IOrchardHelper Orchard
@addTagHelper *, OrchardCore.Media

@functions {
    [FromRoute]
    public string Name { get; set; }
}

@{
	var artists = await Orchard.GetContentItemByAliasAsync("alias:artists");
}

@if (!string.IsNullOrWhiteSpace(Name))
{
    <partial name="ArtistPage" model="Name" />
} else
{   
    @(artists.Content)
    <ul>
        
    </ul>
}
